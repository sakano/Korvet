FILE_INCLUDE_GUARD

ClassExtends(FileNotFoundError, IOError)
{
    Constructor(filename:string, message:string+, internalError:Error? = null) {
        checkArgs
        if (message === void) {
            message = expandMessage(ERROR_FILE_NOT_FOUND, "FILE", filename);
        }
        super.IOError(filename, message, internalError);
    }
}
